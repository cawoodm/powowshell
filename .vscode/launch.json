    "version": "0.2.0",
    "configurations": [
    {
        "type": "node",
        "request": "launch",
        "name": "IDE (electron)",
        "runtimeExecutable": "electron",
        "args" : ["."]
    },

        {
            "type": "node",
            "request": "launch",
            "name": "powTests",
            "program": "${workspaceFolder}\\tests\\powTests.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Current File",
            "program": "${file}"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceFolder}\\ide\\app.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch tmp.js",
            "program": "${workspaceFolder}\\ide\\tmp.js"
        }
    ]
}