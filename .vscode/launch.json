{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "IDE (electron)",
            "runtimeExecutable": "electron",
            "args": [
                "."
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "powTests",
            "program": "${workspaceFolder}\\tests\\powTests.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Current JS",
            "program": "${file}"
        },
        {
            "type": "PowerShell",
            "request": "launch",
            "name": "Launch Current PS",
            "script": "${file}",
            "args": []
        },
        {
            "type": "PowerShell",
            "request": "launch",
            "name": "Launch tmp.ps1",
            "script": "${workspaceFolder}\\bak\\tmp.ps1",
            "args": []
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceFolder}\\ide\\app.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch tmp.js",
            "program": "${workspaceFolder}\\ide\\tmp.js"
        }
    ]
}